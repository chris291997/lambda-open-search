{
  "name": "serverless",
  "version": "1.0.0",
  "description": "docker - serverless - ts-node",
  "scripts": {
    "start": "docker-compose up -d",
    "stop": "docker-compose stop",
    "lint": "eslint --ext .ts src/ --fix",
    "dynamo:migrate": "ts-node -r dotenv/config dynamodb_migration.ts",
    "serve:api": "sls offline -c serverless.yml",
    "serve:api-offline": "sls offline -c serverless-offline.yml",
    "test": "jest --silent"
  },
  "author": "caveman-ch",
  "license": "ISC",
  "devDependencies": {
    "@types/faker": "^5.5.9",
    "@types/jest": "^26.0.20",
