    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "axios": "^1.7.0",
    "dot-object": "^2.1.4",
    "dotenv": "^8.2.0",
    "joi": "^17.3.0",
    "moment-timezone": "^0.5.34",
    "serverless-api-compression": "^1.0.1",
    "serverless-dotenv-plugin": "^3.10.0",
    "serverless-plugin-typescript": "^2.1.5",
    "typeorm": "^0.2.41",
    "typescript": "^4.9.5",
    "uuid": "^9.0.1"
  }
