    "aws-sdk": "^2.1048.0",
    "chai-json-schema": "^1.5.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "jest": "^26.6.3",
    "pluralize": "^8.0.0",
    "prettier": "^2.4.1",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-offline": "^13.5.1",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "yargs": "^16.2.0"
  },
  "dependencies": {
    "@opensearch-project/opensearch": "^2.8.0",
